
<%- include("partials/header"); -%>
    <h1 class="compose">Compose</h1>
    <form class="compose" action="/compose" method="post">
      <div class="form-group">
        <label>Title/Date</label>
        <input class="form-control" type="text" name="postTitle">
        <label>Post</label>
        <textarea class="form-control" name="postBody" rows="5" cols="30"></textarea>
      </div>
      <button class="btn btn-compose btn-primary" type="submit" name="button">Publish</button>
    </form>

<%- include("partials/footer"); -%>
